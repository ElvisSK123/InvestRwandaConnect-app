{
  "name": "Analytics",
  "type": "object",
  "properties": {
    "listing_id": {
      "type": "string",
      "description": "Reference to listing"
    },
    "seller_id": {
      "type": "string",
      "description": "Reference to seller"
    },
    "date": {
      "type": "string",
      "format": "date",
      "description": "Analytics date"
    },
    "views": {
      "type": "number",
      "default": 0
    },
    "unique_visitors": {
      "type": "number",
      "default": 0
    },
    "inquiries": {
      "type": "number",
      "default": 0
    },
    "messages": {
      "type": "number",
      "default": 0
    },
    "favorites": {
      "type": "number",
      "default": 0
    },
    "visitor_countries": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "conversion_rate": {
      "type": "number",
      "description": "Views to inquiries percentage"
    },
    "avg_time_on_page": {
      "type": "number",
      "description": "Average seconds on listing page"
    }
  },
  "required": [
    "listing_id",
    "seller_id",
    "date"
  ]
}